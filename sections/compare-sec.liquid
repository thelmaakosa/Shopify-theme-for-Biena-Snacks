{% schema %}
  {
    "name": "Image with text",
    "settings": [
{
				"type": "text",
				"id": "sec_heading",
				"label": "Section Heading"
			},{
				"type": "image_picker",
				"id": "img_",
				"label": "Section Image"
			},{
				"type": "html",
				"id": "desktop_text",
				"label": "Desktop Table (Html)"
			},{
				"type": "html",
				"id": "mobile_text",
				"label": "Mobile Table(HTML)"
			},{
				"type": "url",
				"id": "btn_url",
				"label": "Button url"
			},{
				"type": "text",
				"id": "button_txt",
				"label": "Button Text"
			}

]
  }
{% endschema %}

