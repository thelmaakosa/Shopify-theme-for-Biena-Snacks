<!--dev section-->
<section class="compare-sec">
	<div class="inner-compare-sec ">
      
		<div class="text-box">
          
          <div class="inner-text-box compare-desktop-sec">
				<h2>{{ section.settings.sec_heading }}</h2>
            
            
               {{ section.settings.desktop_text }}
            
           <div class="btn_cont">
                  <a href="{{ section.settings.btn_url }}" class="btn">{{ section.settings.button_txt }}</a>
              </div>
          </div> 
			<div class="inner-text-box compare-mobile-sec">
				<h2>{{ section.settings.sec_heading }}</h2>
              
              
              
              {{ section.settings.mobile_text }}
              
          	
              
				
              
              
              

              <div class="btn_cont">
                  <a href="{{ section.settings.btn_url }}" class="btn">{{ section.settings.button_txt }}</a>
              </div>
              </div>
		</div>
		<div class="img-box">
			<img src="{{ section.settings.img_ | img_url:'master' }}">
		</div>
	</div>
</section>
